   "source": [
    "def gcd(a, b):\n",
    "    if a==0:\n",
    "        return b\n",
    "    elif b==0:\n",
    "        return a\n",
    "    else:\n",
    "        if a>b:\n",
    "            return gcd(b, a%b)\n",
    "        else:\n",
    "            return gcd(a, b%a)"
   ]
